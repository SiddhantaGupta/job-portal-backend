{
  "moduleFileExtensions": [
    "js",
    "json",
    "ts"
  ],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "moduleNameMapper": {
    "@libs/boat/(.*)": "<rootDir>/../libs/boat/src/$1",
    "@libs/boat": "<rootDir>/../libs/boat/src",
    "@libs/cache/(.*)": "<rootDir>/../libs/cache/src/$1",
    "@libs/cache": "<rootDir>/../libs/cache/src",
    "@libs/nest-mailman/(.*)": "<rootDir>/../libs/nest-mailman/src/$1",
    "@libs/nest-mailman": "<rootDir>/../libs/nest-mailman/src",
    "@libs/nest-queue-redis/(.*)": "<rootDir>/../libs/nest-queue-redis/src/$1",
    "@libs/nest-queue-redis": "<rootDir>/../libs/nest-queue-redis/src",
    "@libs/sq-nest-queue/(.*)": "<rootDir>/../libs/sq-nest-queue/src/$1",
    "@libs/sq-nest-queue": "<rootDir>/../libs/sq-nest-queue/src"
  }
}